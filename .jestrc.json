{
  "testMatch": [
    "**/?(*.)(spec).js?(x)"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "vue"
  ],
  "clearMocks": true,
  "resetMocks": true,
  "resetModules": true,
  "transform": {
    ".*": "<rootDir>/node_modules/webpack-babel-jest"
  },
  "moduleNameMapper": {
    "^vue$": "vue/dist/vue.common.js",
    "@/": "<rootDir>/src/",
    "^common(.*)$": "<rootDir>/src/common$1",
    "^vuex\\+$": "vuex-plus"
  }
}
